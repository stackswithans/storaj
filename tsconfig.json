{
    "compilerOptions": {
        "module": "esnext",
        "target": "esnext",
        "strict": true,
        "removeComments": true,
        "noImplicitAny": true,
        "sourceMap": true,
        "outDir": "./dist/",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true
    },
    "include": ["./src/**/*"],
    "exclude": ["node_modules/*", "**/*.spec.ts"]
}
