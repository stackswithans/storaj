{
  "name": "storaj",
  "version": "0.13.0",
  "description": "A simple lightweight object storage library",
  "main": "./dist/main.js", 
  "types": "./dist/main.d.ts", 
  "homepage":"https://github.com/stackswithans/storaj", 
  "scripts": {
    "test": "ts-node ./tests/test.ts",
    "build": "tsc",
    "cli": "ts-node ./cli.ts", 
    "check": "tsc --noEmit", 
    "prepare":"npm run test && npm run build"
  },  
  "exports": {
    ".": {
        "import": "./dist/main.js"
    }, 
    "./qoperators": {
        "import": "./dist/qoperators.js"
    }
  }, 
  "files": ["/dist/*"], 
  "keywords": [
    "storage",
    "database",
    "library",
    "in-memory store", 
    "backend"
  ],
  "author": "stackwithans",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.8",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
